<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="GRRegression" parallel="tests" thread-count="3" data-provider-thread-count="1">
	<parameter name="Env" value="prod"></parameter>
	<parameter name="Brand" value="FG"></parameter>
	
	<test name="GR Chrome">
		<parameter name="browserName" value="Chrome"></parameter>
		<classes>
			<class name="com.generic.tests.FG.PDP.GRBase" />
		</classes>
	</test><!-- Test -->
	
	<test name="GR iPad">
		<parameter name="browserName" value="mobile_iPad"></parameter>
		<classes>
			<class name="com.generic.tests.FG.PDP.GRBase" />
		</classes>
	</test><!-- Test -->
	<test name="GR iPhone">
		<parameter name="browserName" value="mobile_iPhone X"></parameter>
		<classes>
			<class name="com.generic.tests.FG.PDP.GRBase" />
		</classes>
	</test><!-- Test -->
</suite> <!-- Suite -->