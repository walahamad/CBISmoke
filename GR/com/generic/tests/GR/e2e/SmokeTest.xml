<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="e2e Guest Regression" parallel="tests" thread-count="3" data-provider-thread-count="1">

	<test name="e2e chrome">
		<parameter name="browserName" value="chrome"></parameter>
		<parameter name="Env" value="www"></parameter>
		<parameter name="Brand" value="GR"></parameter>

		<classes>
			<class name="com.generic.tests.GR.e2e.SmokeTest_e2e" />
		</classes>
	</test>

	<test name="e2e iPad">
		<parameter name="browserName" value="mobile_iPad"></parameter>
		<parameter name="Env" value="www"></parameter>
		<parameter name="Brand" value="GR"></parameter>
		<classes>
			<class name="com.generic.tests.GR.e2e.SmokeTest_e2e" />
		</classes>
	</test>


	<test name="e2e iPhone">
		<parameter name="browserName" value="mobile_iPhone X"></parameter>
		<parameter name="Env" value="www"></parameter>
		<parameter name="Brand" value="GR"></parameter>
		<classes>
			<class name="com.generic.tests.GR.e2e.SmokeTest_e2e" />
		</classes>
	</test> 	
	

</suite> <!-- Suite -->