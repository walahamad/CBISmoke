<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="cart regression" parallel="tests" thread-count="3"
	data-provider-thread-count="1">

	<test name="cart Tests _chrome">
		<parameter name="browserName" value="chrome"></parameter>
		<parameter name="Env" value="prod"></parameter>
		<parameter name="Brand" value="GR"></parameter>
		<classes>
			<class name="com.generic.tests.GR.Cart.CartBase" />

		</classes>
	</test>

	<test name="cart iPad FG">
		<parameter name="browserName" value="mobile_iPad"></parameter>
		<parameter name="Env" value="prod"></parameter>
		<parameter name="Brand" value="GR"></parameter>
		<classes>
			<class name="com.generic.tests.GR.Cart.CartBase" />
		</classes>
	</test>

	<test name="cart iPhone">
		<parameter name="browserName" value="mobile_iPhone X"></parameter>
		<parameter name="Env" value="prod"></parameter>
		<parameter name="Brand" value="GR"></parameter>
		<classes>
			<class name="com.generic.tests.GR.Cart.CartBase" />
		</classes>
	</test>

</suite>  