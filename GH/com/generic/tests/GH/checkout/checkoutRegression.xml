<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="checkout Regression" parallel="tests" thread-count="10" data-provider-thread-count="2">

	<test name="checkout chrome">
		<parameter name="browserName" value="chrome"></parameter>
		<parameter name="Env" value="www"></parameter>
        <parameter name="Brand" value="FG"></parameter>
		
		<classes>
			<class name="com.generic.tests.FG.checkout.Base_checkout" />
		</classes>
		</test> 
			
	<test name="Registration Tests_iPad">
		<parameter name="browserName" value="mobile_iPad"></parameter>
		<parameter name="Env" value="www"></parameter>
        <parameter name="Brand" value="FG"></parameter>
		<classes>
			<class name="com.generic.tests.FG.checkout.Base_checkout" />
		</classes>
	</test> 
      
	
	<test name="Registration iPhone">
		<parameter name="browserName" value="mobile_iPhone X"></parameter>
		<parameter name="Env" value="www"></parameter>
        <parameter name="Brand" value="FG"></parameter>
		<classes>
			<class name="com.generic.tests.FG.checkout.Base_checkout" />
		</classes>
		</test> 
		
	

</suite> <!-- Suite -->